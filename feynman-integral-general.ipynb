   "source": [
    "from sympy import symbols,simplify,integrate\n",
    "A, B, x = symbols('A B x')\n",
    "integrand = 1/(x*A + (1-x)*B)**2\n",
    "result=integrate(integrand,(x,0,1))\n",
    "simplify(result)"
   ]
